{
  "name": "@rarible/hardhat-v3-deploy",
  "version": "0.9.0",
  "directories": {
    "test": "test"
  },
  "type": "module",
  "scripts": {
    "build": "yarn prettier && yarn lint && hardhat clean && hardhat compile && yarn prettier:ts && tsc",
    "lint": "solhint 'contracts/**/*.sol' --config ../../.solhintrc.json",
    "lint:fix": "solhint 'contracts/**/*.sol' --fix --config ../../.solhintrc.json",
    "prettier": "prettier --write --plugin=prettier-plugin-solidity 'contracts/**/*.sol' --config ../../.prettierrc.json",
    "prettier:ts": "prettier --write 'test/**/*.ts' --config ../../.prettierrc.json",
    "test": "npx hardhat test",
    "verify:etherscan": "npx rocketh-verify -e sepolia etherscan",
    "verify:sourcify": "npx rocketh-verify -e sepolia sourcify",
    "verify:blockscout": "npx rocketh-verify -e sepolia blockscout",
    "acceptance-test:sepolia": "npx tsx scripts/acceptance-test-sepolia.ts"
  },
  "license": "MIT",
  "dependencies": {
    "@openzeppelin/contracts": "^5.4.0",
    "@openzeppelin/contracts-upgradeable": "^5.4.0",

{
  "name": "@rarible/packs",
  "version": "0.9.0",
  "description": "Solidity operator role functionality",
  "type": "module",
  "files": [
    "contracts",
    "readme.md",
    "dist"
  ],
  "access": "public",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rariblecom/protocol-contracts.git"
  },
  "keywords": [
    "ethereum",
    "truffle",
    "test"
  ],
  "author": "0v1se",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rariblecom/protocol-contracts/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn prettier && yarn lint && hardhat clean && hardhat compile && yarn prettier:ts && tsc",
    "lint": "solhint 'contracts/**/*.sol' --config ../../.solhintrc.json",
    "lint:fix": "solhint 'contracts/**/*.sol' --fix --config ../../.solhintrc.json",
    "prettier": "prettier --write --plugin=prettier-plugin-solidity 'contracts/**/*.sol' --config ../../.prettierrc.json",
    "prettier:ts": "prettier --write 'test/**/*.ts' 'ignition/**/*.ts' --config ../../.prettierrc.json",
    "test": "npx hardhat test --network hardhatMainnet",
    "deploy:raripack:base": "npx hardhat run scripts/deploy-raripack.ts --network base",
    "deploy:raripack:sepolia": "npx hardhat run scripts/deploy-raripack.ts --network sepolia",
    "deploy-collections:base": "npx hardhat run scripts/deploy-collections.ts --network base",
    "deploy-collections:sepolia": "npx hardhat run scripts/deploy-collections.ts --network sepolia",
    "deploy:nft-infra:base": "npx hardhat run scripts/deploy-nft-infrastructure.ts --network base",
    "deploy:nft-infra:sepolia": "npx hardhat run scripts/deploy-nft-infrastructure.ts --network sepolia",
    "process-collections:base": "npx hardhat run scripts/process-collections.ts --network base",
    "process-collections:sepolia": "npx hardhat run scripts/process-collections.ts --network sepolia",
    "export-collections:base": "npx hardhat run scripts/export-collections.ts --network base",
    "export-collections:sepolia": "npx hardhat run scripts/export-collections.ts --network sepolia",
    "upgrade:base": "npx hardhat run scripts/upgrade-base.ts --network base",
    "upgrade:sepolia": "npx hardhat run scripts/upgrade-sepolia.ts --network sepolia"
  },
  "gitHead": "9830dc5f40a9cb462b8a46097d4e452e456e3b41",
  "dependencies": {
    "@openzeppelin/contracts-upgradeable": "^5.4.0",
    "@openzeppelin/contracts": "^5.4.0",
    "@chainlink/contracts": "^1.5.0",
    "@rarible/test": "^0.9.0",
    "erc721a": "^4.3.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "js-yaml": "^4.1.0",
    "@nomicfoundation/hardhat-ethers": "^4.0.3",
    "@nomicfoundation/hardhat-ethers-chai-matchers": "^3.0.2",
    "@nomicfoundation/hardhat-ignition": "^3.0.5",
    "@nomicfoundation/hardhat-ignition-ethers": "^3.0.5",
    "@nomicfoundation/hardhat-keystore": "^3.0.3",
    "@nomicfoundation/hardhat-mocha": "^3.0.7",
    "@nomicfoundation/hardhat-network-helpers": "^3.0.3",
    "@nomicfoundation/hardhat-toolbox-mocha-ethers": "^3.0.1",
    "@nomicfoundation/hardhat-typechain": "^3.0.1",
    "@nomicfoundation/hardhat-verify": "^3.0.7",
    "@types/chai": "^4.3.20",
    "@types/chai-as-promised": "^8.0.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.19.1",
    "chai": "^5.3.3",
    "ethers": "^6.15.0",
    "forge-std": "github:foundry-rs/forge-std#v1.9.4",
    "hardhat": "^3.0.15",
    "mocha": "^11.7.5",
    "tsx": "^4.19.0",
    "typescript": "~5.8.0",
    "solhint": "^6.0.1",
    "prettier-plugin-solidity": "^2.2.0",
    "prettier": "^3.6.2"
  }
}
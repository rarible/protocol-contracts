{
    "plugins": ["prettier-plugin-solidity"],
    "overrides": [
      {
        "files": "*.sol",
        "options": {
          "parser": "slang",
          "printWidth": 80,
          "tabWidth": 4,
          "useTabs": false,
          "singleQuote": false,
          "bracketSpacing": false
        }
      }
    ]
  }
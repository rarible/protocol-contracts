  "scripts": {
    "test": "truffle test",
    "build": "truffle compile --all && typechain --target ethers-v5 'build/contracts/*.json' && tsc -skipLibCheck",
    "compile-zk": "npx hardhat --config zk.hardhat.config.ts compile",
    "deploy-factory-721-zk": "npx hardhat run --config zk.hardhat.config.ts scripts/zk-deploy-721-factory.ts",
    "verify-factory-721-zk": "npx hardhat verify --config zk.hardhat.config.ts 0x5F577Dd877b0136ab7ed8A4d7b303aD38348a1Ea '0x9301194711E0015316AA0d9e0F184dFB87f70582' '0x85a26E6D52239817570Ff643bA09E3AA5393A805' '0x11983886da3c379E507A874649C96D7EEd086c32'",
